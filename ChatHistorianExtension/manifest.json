{
  "manifest_version": 3,
  "name": "ChatGPT Navigation Button",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "css": [
        "styles.css"
      ]
    }
  ]
}