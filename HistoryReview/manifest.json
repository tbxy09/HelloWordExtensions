{
  "manifest_version": 3,
  "name": "History Review",
  "version": "1.0",
  "description": "A description of my Chrome extension",
  "permissions": [
    "webNavigation",
    "history",
    "storage",
    "tabs",
    "commands"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "open_dev_tools": {
      "suggested_key": {
        "default": "Alt+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Open Chrome Developer Tools"
    }
  },
  "chrome_url_overrides": {
    "history": "history.html"
  }
}