{
  "manifest_version": 3,
  "name": "History Review",
  "version": "1.0",
  "description": "A description of my Chrome extension",
  "minimum_chrome_version": "118",
  "permissions": [
    "webNavigation",
    "scripting",
    "history",
    "storage",
    "tabs",
    "commands",
    "activeTab",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/keymaster.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "execute_move_tab_to_window": {
      "suggested_key": {
        "default": "Alt+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Move tab to a specific window"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "side_panel.html",
        "side_panel.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}